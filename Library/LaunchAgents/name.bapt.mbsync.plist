<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>name.bapt.mbsync</string>
    <key>ProgramArguments</key>
    <array>
      <string>/Users/baptiste/.config/neomutt/sync</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartInterval</key>
    <integer>180</integer>
    <key>StandardOutPath</key>
    <string>/Users/baptiste/Mail/.mailsync.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/baptiste/Mail/.mailsync.log</string>
  </dict>
</plist>
